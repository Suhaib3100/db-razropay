{
    "name": "your/project-name",
    "description": "Your project description",
    "type": "project",
    "require": {
        "dagar/bagisto-razorpay": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Your\\Namespace\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bagisto\\Razorpay\\Providers\\RazorpayServiceProvider"
            ],
            "aliases": {},
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan package:discover --ansi"
        ]
    }
}
